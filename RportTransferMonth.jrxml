	<queryString>
		<![CDATA[SELECT ClassRoomName, 
CASE Gender 
WHEN 0 THEN 'ນາງ' 
WHEN 1 THEN 'ທ້າວ' 
ELSE 'Unknown' 
END AS GenderLabel,StudentName,PaymentDate,TransferTime,Money,FinancialMonth,FinancialComment
FROM tb_financial 
INNER JOIN tb_register ON tb_financial.RegisterID = tb_register.registerID 
INNER JOIN tb_student ON tb_financial.StudentID = tb_student.StudentID
WHERE FinancialMonth=$P{parmMonth} AND yearID=$P{parmYearID} AND FinancialType='1';]]>
	</queryString>
